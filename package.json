{
  "name": "sms-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "babel-node ./src/index.js",
    "test": "PORT=2081 NODE_ENV=test babel-node ./node_modules/mocha/bin/_mocha --exit",
    "build": "babel src -d lib --source-maps --copy-files",
    "start:prod": "node ./lib/index.js",
    "knex": "babel-node node_modules/.bin/knex --knexfile=./src/db/knexfile.js",
    "knex:prod": "node node_modules/.bin/knex --knexfile=./lib/db/knexfile.js",
    "db:migrate": "npm run migrate:rollback && npm run migrate:latest && npm run seed:run",
    "db:prod": "npm run knex:prod migrate:rollback && npm run knex:prod migrate:latest && npm run knex:prod seed:run",
    "migrate:latest": "npm run knex migrate:latest",
    "migrate:rollback": "npm run knex migrate:rollback",
    "seed:run": "npm run knex seed:run"
  },
  "keywords": [
    "backend",
    "sms",
    "REST",
    "API"
  ],
  "author": "appshop",
  "homepage": "https://github.com/AppDevelopmentShop/sms-service",
  "engines": {
    "node": "10.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:AppDevelopmentShop/sms-service.git"
  },
  "devDependencies": {
    "@types/node": "^10.0.8",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-node8": "^1.2.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "mocha": "^5.1.1",
    "sinon": "^5.0.7"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "ioredis": "^3.2.2",
    "joi": "^13.3.0",
    "knex": "^0.14.6",
    "knexnest": "^0.4.1",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.4.0",
    "libphonenumber-js": "^1.2.6",
    "pg": "^7.4.3",
    "twilio": "^3.17.0",
    "winston": "^2.4.2"
  }
}
